<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane prefHeight="300.0" prefWidth="300.0" styleClass="background" stylesheets="@base16-google-dark.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="br.com.ufrpeuag.gastromaster.ui.TelaProdutoCadastroControlador">
   <children>
      <Label text="Produto" AnchorPane.leftAnchor="5.0" AnchorPane.topAnchor="5.0" />
      <Label text="Nome" AnchorPane.leftAnchor="10.0" AnchorPane.topAnchor="58.0" />
      <Label text="Preço" AnchorPane.leftAnchor="10.0" AnchorPane.topAnchor="108.0" />
      <Label text="Quantidade" AnchorPane.leftAnchor="10.0" AnchorPane.topAnchor="158.0" />
      <TextField fx:id="nomeFieldProduto" promptText="Nome do produto" AnchorPane.rightAnchor="20.0" AnchorPane.topAnchor="50.0" />
      <TextField fx:id="precoFieldProduto" prefHeight="37.0" prefWidth="159.0" promptText="Preço" AnchorPane.rightAnchor="20.0" AnchorPane.topAnchor="100.0" />
      <TextField fx:id="quantidadeFieldProduto" prefHeight="37.0" prefWidth="159.0" promptText="Quantidade" AnchorPane.rightAnchor="20.0" AnchorPane.topAnchor="150.0" />
      <HBox spacing="10.0" AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="30.0">
         <children>
            <Button fx:id="concluirProdutoCadastro" mnemonicParsing="false" onAction="#handleConcluirCadastro" text="Concluir" />
         </children>
      </HBox>
   </children>
</AnchorPane>
