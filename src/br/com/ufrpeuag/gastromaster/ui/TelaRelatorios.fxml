      <Button fx:id="gerarGrafico" layoutX="10.0" layoutY="143.0" mnemonicParsing="false" onAction="#handleGerarGrafico" text="Gerar Gráfico" AnchorPane.leftAnchor="10.0" AnchorPane.topAnchor="143.0" />
      <Button fx:id="gerarGraficoAnual" layoutX="10.0" layoutY="361.0" mnemonicParsing="false" onAction="#handleGerarGraficoAnual" text="Gráfico Anual" />
